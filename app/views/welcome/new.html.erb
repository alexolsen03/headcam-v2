<%= form_for @location do |f| %>
	<%= f.label :address %>
	<%= f.text_field :address %>

	<%= f.label :latitude %>
	<%= f.text_field :latitude %>

	<%= f.label :longitude %>
	<%= f.text_field :longitude %>

	<%= f.label :country_code %>
	<%= f.text_field :country_code %>

	<%= f.submit "Create" %>
<% end %>